# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'miit' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'Alamofire', '~> 4.7'
  pod 'SwiftyJSON', '~> 4.1'
  pod 'SnapKit', '~> 4.0.0'
  pod 'GoogleWebRTC'
  pod 'AsyncSwift'
  pod 'UICircularProgressRing'
  pod 'SwifterSwift'
  pod 'ZoomImageView'

  target 'miitTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'miitUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
